    "autoload": {
        "psr-0": {
            "Pantarei\\Bundle\\BootstrapBundle": ""
        }
    },
    "description": "Symfony BootstrapBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "homepage": "https://github.com/pantarei/bootstrap-bundle",
    "keywords": [
        "bootstrap",
        "css",
        "symfony"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "pantarei/bootstrap-bundle",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "sensio/framework-extra-bundle": "2.3.*",
        "symfony/browser-kit": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/css-selector": "2.3.*",
        "symfony/dependency-injection": "2.3.*",
        "symfony/finder": "2.3.*",
        "symfony/form": "2.3.*",
        "symfony/twig-bridge": "2.3.*",
        "symfony/twig-bundle": "2.3.*",
        "symfony/validator": "2.3.*",
        "symfony/yaml": "2.3.*",
        "twig/twig": "~1.11"
    },
    "suggest": {
        "leafo/lessphp": "lessphp is a compiler for LESS written in PHP",
        "twitter/bootstrap": "Sleek, intuitive, and powerful front-end framework for faster and easier web development"
    },
    "target-dir": "Pantarei/Bundle/BootstrapBundle",
    "type": "symfony-bundle"
}
