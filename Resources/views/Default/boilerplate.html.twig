<!DOCTYPE html>
<!--[if IE 8]> <html class="no-js lt-ie9" {{ block('html_attributes') }}> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" {% block html_attributes %}{% endblock %} {% block rdf_namespaces %}{% endblock %}> <!--<![endif]-->
  <head>
    {% block head %}{% endblock %}
    <title>{% block head_title %}Bootstrap 101 Template{% endblock %}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="{{ asset('bundles/twitter/bootstrap/dist/css/bootstrap.min.css') }}">
    {% block styles %}{% endblock %}

    <script src="{{ asset('bundles/twitter/bootstrap/assets/js/jquery.js') }}"></script>
    <script src="{{ asset('bundles/twitter/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/twitter/bootstrap/assets/js/holder/holder.js') }}"></script>
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/twitter/bootstrap/assets/js/html5shiv.js') }}"></script>
    <script src="{{ asset('bundles/twitter/bootstrap/assets/js/respond/respond.min.js') }}"></script>
    <![endif]-->
    {% block scripts %}{% endblock %}
  </head>
  <body class="{% block classes %}{% endblock %}" {% block attributes %}{% endblock %} >
    {% block page_top %}{% endblock %}
    {% block page %}
    <h1>Hello, world!</h1>
    {% endblock %}
    {% block page_bottom %}{% endblock %}
  </body>
</html>
