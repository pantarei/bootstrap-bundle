#!/bin/bash

rm -rf Tests/app/cache
rm -rf Tests/app/log

COMMIT=`git log --oneline -n 1`
if [ -z "$COMMIT" ];
then
        exit 1
fi

cd build/_gh_pages
git rm -rf * >/dev/null 2>&1

cd ../../
doxygen config.doxy >/dev/null 2>&1

cd build/_gh_pages
git add * >/dev/null 2>&1
git commit -am "$COMMIT"
git push
git pull

cd ../../
git pull
